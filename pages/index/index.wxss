/* 转盘 */
.canvas-container {
  margin: 0 auto;
  position: relative;
  width: 600rpx;
  height: 600rpx;
  border-radius: 50%;
  box-shadow: 0 4rpx 6rpx #333, 0 0 4rpx #000;
}
.canvas-content {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
  display: block;
  width: 600rpx;
  height: 600rpx;
  border-radius: inherit;
  background-clip: padding-box;
  background-color: #FFD700;
  overflow: hidden;




}
.canvas-element {
  position: relative;
  z-index: 1;
  width: inherit;
  height: inherit;
  border-radius: 50%;
}
.canvas-list {
  position: absolute;
  left: 0;
  top: 0;
  width: inherit;
  height: inherit;
  z-index: 10;
}
.canvas-item {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  color: black;
  font-weight: bold;
  text-shadow: 0 2rpx 2rpx rgba(255, 255, 255, 0.6);
}
.canvas-item-text {
  position: relative;
  display: block;
  padding-top: 40rpx;
  margin: 0 auto;
  text-align: center;
  width: fit-content;
  -webkit-transform-origin: 50% 300rpx;
  transform-origin: 50% 300rpx;

}

/* 分隔线 */
.canvas-line {
  position: absolute;
  left: 0;
  top: 0;
  width: inherit;
  height: inherit;
  z-index: 9;
}
.canvas-litem {
  position: absolute;
  left: 300rpx;
  top: 0;
  width: 2rpx;
  height: 300rpx;
  background-color: rgba(7, 7, 7, 0.603);
  overflow: hidden;
  -webkit-transform-origin: 50% 300rpx;
  transform-origin: 50% 300rpx;
}

/**
* 抽奖按钮
*/
.canvas-btn {
  position: absolute;
  left: 220rpx;
  top: 220rpx;
  z-index: 400;
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  color: #f4e9cc;
  background-color: #FA0605;
  line-height: 160rpx;
  text-align: center;
  font-size: 40rpx;
  text-shadow: 0 -2rpx 2rpx rgba(0, 0, 0, 0.6);
  box-shadow: 0 6rpx 10rpx rgba(0, 0, 0, 0.6);
  text-decoration: none;
}
.canvas-btn::after {
  position: absolute;
  display: block;
  content: ' ';
  left: 20rpx;
  top: -92rpx;
  width: 0;
  height: 0;
  overflow: hidden;
  border-width: 60rpx;
  border-style: solid;
  border-color: transparent;
  border-bottom-color: #FA0605;
}
.canvas-btn.disabled {
  pointer-events: none;
  background: #b07a7b;
  color: #ccc;
}
.canvas-btn.disabled::after {
  border-bottom-color: #b07a7b;
}
